												{% endif %}
													<i class="fas fa-search" style="color: transparent;"></i>
													<i class="fas fa-retweet nav-icon"></i>
													<p>Experiencia de aprendizaje
													</p>
												</a>


											</li>

										</ul>

									</li>

								{% endif %}


								{% if is_granted('ROLE_USER')%}


									<li class="nav-item has-treeview menu-open">
										{% if 'test' in app.request.uri %} 
									        <a href="#"  class="btn-sm nav-link active">
									    {% else %}
									        <a href="#"  class="btn-sm nav-link">
                                        {% endif %}
											<i class="nav-icon far fa-user"></i>
											<p>
												Estudiantes
												<i class="fas fa-angle-left right"></i>
											</p>
										</a>
										<ul class="nav nav-treeview">

											<li class="nav-item">
											    {% if is_granted('ROLE_ADMIN') %}
    											    {% if 'test' in app.request.uri %} 
    												<a href="{{ path('app_model_diagram_test_index') }}" class="btn-m nav-link active">
    												{% else %}
    												<a href="{{ path('app_model_diagram_test_index') }}" class="btn-m nav-link">
    												{% endif %}
    													<i class="fas fa-search" style="color: transparent;"></i>
    													<i class="fa fa-server nav-icon"></i>
    												<p>Actividades Resueltas</p>
    												</a>
    											{% else %}
    											    {% if 'test' in app.request.uri %} 
    												<a href="{{ path('app_model_diagram_test_index2', {'id': app.user.id}) }}" class="btn-m nav-link active">
    												{% else %}
    												<a href="{{ path('app_model_diagram_test_index2', {'id': app.user.id}) }}" class="btn-m nav-link">
    												{% endif %}
    													<i class="fas fa-search" style="color: transparent;"></i>
    													<i class="fa fa-server nav-icon"></i>
    												<p>Mis Actividades</p>
    												</a>
    											{% endif %}
											</li>

											<li class="nav-item"> 

												<a href="/public/GoJS-master/projects/pdf/diagrama.php?idUser={{app.user.id}}" class="btn-m nav-link">
													<i class="fas fa-search" style="color: transparent;"></i>
													<i class="fa fa-plus-circle  nav-icon"></i>
													<p>Responder Actividad</p>
												</a>
											</li>

										</ul>
									</li>
								{% endif %}
							</ul>
						</nav>
						<!-- /.sidebar-menu -->
					</div>
					<!-- /.sidebar -->
				{% endif %}
				</aside>


			{% endblock %}

		</div>
