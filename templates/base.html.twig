	<body>
		<!--class="hold-transition sidebar" style="margin: 0;"-->
		<!-- Site wrapper -->
		<div class="wrapper">
			{% block navbar %}
				{% include 'user-nav.html.twig' %}
			{% endblock %}


			{% block sidebar %}
				<!-- Main Sidebar Container -->
				<aside
					class="main-sidebar sidebar-dark-primary elevation-4"  style="width: 280px;">
				    {% if app.user %}
					<!-- Brand Logo -->
					<a href="{{ path('app_begin')}}" class="brand-link">
						<img src="{{asset('dist/img/XperienceUML.png')}}" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
						<span class="brand-text font-weight-light">
							<i>XPerienceUML</i>
						</span>
					</a>

					<!-- Sidebar -->
					<div
						class="sidebar">
						<!-- Sidebar user (optional) -->
						
							<div class="user-panel mt-3 pb-3 mb-3 d-flex">
								<div class="image">
									<img src="{{asset('dist/img/avatar5.png')}}" class="img-circle elevation-2" alt="User Image">
								</div>
								<div class="info">
									<a href="{{path('app_user_show' ,{'id':app.user.id}) }}" class="d-block">{{ app.user.name }}</a>
								</div>
							</div>
						
						<!-- Sidebar Menu -->
						<nav class="mt-2">
							<ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
								<!-- Add icons to the links using the .nav-icon class with font-awesome or any other icon font library -->

								{% if is_granted('ROLE_ADMIN') %}

									<li class="nav-item has-treeview menu-open">
									    {% if not('test' in app.request.uri) and not('xapi' in app.request.uri) %} 
									        <a href="#"  class="btn-m nav-link active">
									    {% else %}
									        <a href="#"  class="btn-m nav-link">
                                        {% endif %}
                                        
											<i class="nav-icon fas fa-wrench"></i>
											<p>
												Gestión de cursos
												<i class="fas fa-angle-left right"></i>

											</p>
										</a>
										<ul class="nav nav-treeview">
										    <li class="nav-item">
											    {% if ('course/' in app.request.uri) %} 
												    <a href="{{ path('app_course_index') }}" class="btn-m nav-link active">
												{% else %}
												    <a href="{{ path('app_course_index') }}" class="btn-m nav-link">
												{% endif %}
													    <i class="fas fa-search" style="color: transparent;"></i>
													    <i class="fa  fa-graduation-cap nav-icon"></i>
													    <p>Cursos</p>
												    </a>
											</li>
											<li class="nav-item">
											    {% if ('users/' in app.request.uri) %} 
												<a href="{{ path('app_user_index') }}" class="btn-m nav-link active">
												{% else %}
												<a href="{{ path('app_user_index') }}" class="btn-m nav-link">
												{% endif %}
													<i class="fas fa-search" style="color: transparent;"></i>
													<i class="fa fa-users nav-icon"></i>
													<p>Usuarios
													</p>
												</a>
											</li>
											<li class="nav-item">
											    {% if ('activity/' in app.request.uri) %} 
											    <a href="{{ path('app_n_activity_index') }}" class="btn-m nav-link active">
											    {% else %}
												<a href="{{ path('app_n_activity_index') }}" class="btn-m nav-link">
												{% endif %}
												    <i class="fas fa-search" style="color: transparent;"></i>
													<i class="fa fa-graduation-cap nav-icon"></i>
													<p>
														Actividades del Curso</p>
												</a>
											</li>

										
											<li class="nav-item">
